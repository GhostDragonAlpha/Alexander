{
  "desktop_launcher": {
    "default_resolution": "1280x720",
    "default_quality": "medium",
    "default_port": 8080,
    "max_restarts": 3,
    "timeout_seconds": 0,
    "enable_console": true,
    "enable_debug": true,
    "process_check_interval": 10,
    "http_api_timeout": 180
  },
  "screenshot_capture": {
    "capture_interval": 3.0,
    "jpeg_quality": 85,
    "max_screenshots": 1000,
    "auto_cleanup": true,
    "enable_compression": true,
    "metadata_embedding": true,
    "enable_ai_transmission": true,
    "server_endpoint": "/receive_screenshot",
    "resize_to": null,
    "capture_region": null
  },
  "test_scenarios": {
    "default_timeout": 300,
    "max_attempts": 3,
    "required_screenshots": 5,
    "scenario_configs": {
      "first_mission": {
        "timeout": 180,
        "success_criteria": {
          "mission_completed": true,
          "min_distance_traveled": 1000.0,
          "max_time": 120
        },
        "failure_criteria": {
          "ship_destroyed": true,
          "timeout": true,
          "stuck_duration": 30
        }
      },
      "trading_system": {
        "timeout": 240,
        "success_criteria": {
          "trade_completed": true,
          "credits_earned": 100,
          "items_traded": 1
        },
        "failure_criteria": {
          "insufficient_credits": true,
          "trade_rejected": true,
          "timeout": true
        }
      },
      "combat_scenario": {
        "timeout": 300,
        "randomize": true,
        "success_criteria": {
          "enemies_defeated": 3,
          "player_survived": true,
          "combat_duration": 60
        },
        "failure_criteria": {
          "player_destroyed": true,
          "ammo_depleted": true,
          "timeout": true
        }
      },
      "free_exploration": {
        "timeout": 600,
        "randomize": true,
        "success_criteria": {
          "min_distance_explored": 5000.0,
          "discoveries_made": 3,
          "waypoints_visited": 5
        },
        "failure_criteria": {
          "ship_destroyed": true,
          "fuel_depleted": true,
          "timeout": true
        }
      }
    }
  },
  "game_state_extractor": {
    "state_update_interval": 0.1,
    "nearby_actor_detection_radius": 5000.0,
    "track_performance": true,
    "extract_player_state": true,
    "extract_world_state": true,
    "extract_performance_metrics": true,
    "extract_inventory_state": true,
    "extract_mission_state": true,
    "extract_nearby_actors": true,
    "extract_ui_state": true
  },
  "performance_monitoring": {
    "track_fps": true,
    "track_memory": true,
    "track_hardware": true,
    "fps_warning_threshold": 30,
    "memory_warning_threshold_mb": 4096,
    "log_performance_interval": 60
  },
  "streaming_optimization": {
    "texture_streaming": {
      "enabled": true,
      "pool_size_mb": 2048,
      "critical_memory_mip_bias": 2.0,
      "aggressive_pool_size_mb": 1433,
      "balanced_pool_size_mb": 1740,
      "conservative_pool_size_mb": 2048
    },
    "mesh_lod_streaming": {
      "enabled": true,
      "distance_scale": 1.0,
      "critical_memory_lod_bias": 2.0,
      "aggressive_distance_scale": 0.7,
      "balanced_distance_scale": 0.85,
      "conservative_distance_scale": 1.3
    },
    "asset_loading": {
      "async_loading_enabled": true,
      "max_concurrency": 5,
      "critical_memory_concurrency": 2,
      "aggressive_concurrency": 2,
      "balanced_concurrency": 3,
      "conservative_concurrency": 5,
      "priority_thresholds": {
        "critical": 100,
        "high": 75,
        "medium": 50,
        "low": 25
      }
    },
    "level_streaming": {
      "enabled": true,
      "memory_limit_mb": 512,
      "unload_distant_levels": true,
      "streaming_distance_threshold": 50000.0
    },
    "material_instances": {
      "enable_pooling": true,
      "max_instances_per_material": 50,
      "cleanup_interval_seconds": 60
    },
    "memory_thresholds": {
      "warning_mb": 3072,
      "critical_mb": 4096,
      "emergency_mb": 5120
    }
  },
  "http_api_server": {
    "listen_port": 8080,
    "max_requests_per_second": 60,
    "enable_verbose_logging": true,
    "enable_rate_limiting": true,
    "cors_enabled": true,
    "allowed_origins": ["*"],
    "streaming_endpoints": {
      "get_streaming_metrics": "/streaming_metrics",
      "set_streaming_strategy": "/set_streaming_strategy",
      "get_asset_loading_status": "/asset_loading_status",
      "get_texture_memory_stats": "/texture_memory_stats",
      "get_mesh_memory_stats": "/mesh_memory_stats",
      "force_memory_optimization": "/force_memory_optimization"
    },
    "tick_optimization_endpoints": {
      "get_tick_stats": "/tick_stats",
      "set_actor_priority": "/set_actor_priority",
      "get_dormant_actors": "/dormant_actors",
      "reset_tick_optimization": "/reset_tick_optimization"
    }
  },
  "logging": {
    "log_level": "INFO",
    "log_to_file": true,
    "log_to_console": true,
    "max_log_file_size_mb": 100,
    "max_log_files": 10,
    "log_directory": "automation_config/logs"
  },
  "integration": {
    "game_mode": "/Script/Alexander.AutomationGameMode",
    "default_map": "VRTemplateMap",
    "unreal_editor_path": "C:\\Program Files\\Epic Games\\UE_5.6\\Engine\\Binaries\\Win64\\UnrealEditor.exe",
    "project_file": "C:\\Users\\allen\\Desktop\\Alexander\\Alexander\\Alexander.uproject"
  }
}